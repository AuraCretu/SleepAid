<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextBox">
        <attr name="text" format="string" />
        <attr name="textSize" format="float" />
    </declare-styleable>

    <declare-styleable name="SleepDiaryQuestionComponent">
        <attr name="questionText" format="string" />
        <attr name="informationText" format="string" />
    </declare-styleable>

    <declare-styleable name="EditTextAnswerComponent">
        <attr name="hint" format="string" />
        <attr name="inputType" format="string" />
        <attr name="maxLength" format="integer" />
        <attr name="editTextTextSize" format="float" />
    </declare-styleable>

    <declare-styleable name="ErrorMessage">
        <attr name="errorText" format="string" />
        <attr name="errorTextSize" format="float" />
    </declare-styleable>
</resources>